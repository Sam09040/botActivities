{
  "name": "botactivities",
  "version": "1.0.0",
  "description": "Project using Apollo Server and Prisma using TypeScript",
  "main": "index.js",
  "scripts": {
    "compile": "tsc",
    "start": "npm run compile && node ./dist/index.js",
    "dev": "tsx watch src/index.ts",
    "check": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "start_test": "npx dotenvx run --env-file=test.env -- npm run start",
    "test": "npx dotenvx run --env-file=test.env -- npx prisma db push --force-reset && mocha"
  },
  "keywords": [
    "apollo",
    "prisma",
    "ts"
  ],
  "author": "Sam",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@prisma/client": "5.20.0",
    "apollo-server": "3.13.0",
    "fs": "0.0.1-security",
    "graphql": "16.9.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "1.19.2",
    "@eslint/js": "9.11.1",
    "@types/bcrypt": "5.0.2",
    "@types/chai": "5.0.0",
    "@types/mocha": "10.0.9",
    "@types/node": "22.7.4",
    "apollo-server": "3.13.0",
    "axios": "1.7.7",
    "bcrypt": "5.1.1",
    "chai": "5.1.1",
    "dotenv": "16.4.5",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.10.0",
    "graphql": "16.9.0",
    "mocha": "10.7.3",
    "prettier": "3.3.3",
    "prisma": "5.21.0",
    "ts-node": "10.9.2",
    "tsx": "4.19.1",
    "typescript": "5.6.2",
    "typescript-eslint": "8.8.0"
  }
}
